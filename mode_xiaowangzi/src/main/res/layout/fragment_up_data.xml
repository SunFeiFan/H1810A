<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragment.UpDataFragment">


    <Button
        android:id="@+id/okhttp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="okhttp" />

    <Button
        android:id="@+id/retrofit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="84dp"
        android:text="retrofit" />

    <TextView
        android:id="@+id/okhttp_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="55dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/retrofit_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="146dp"
        android:text="TextView" />

    <ImageView
        android:id="@+id/okhttp_image"
        android:layout_width="179dp"
        android:layout_height="126dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:src="@android:color/holo_blue_bright" />

    <ImageView
        android:id="@+id/retrofit_image"
        android:layout_width="181dp"
        android:layout_height="128dp"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/retrofit_tv"
        android:src="@android:color/darker_gray" />

    <Button
        android:id="@+id/guangbo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/retrofit_image"
        android:text="测试广播" />
</RelativeLayout>